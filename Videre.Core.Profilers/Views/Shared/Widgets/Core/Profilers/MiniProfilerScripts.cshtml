@using System.Collections.Generic;
@using Videre.Core.Extensions;
@using Models = Videre.Core.Models;
@using Videre.Core.Services;
@using StackExchange.Profiling

@model Models.Widget


    @if (Security.IsActivityAuthorized("Profiler", "MiniProfiler"))
    {
        var startHidden = Model.GetAttribute("StartHidden", "No") == "Yes";
        @MiniProfiler.RenderIncludes(startHidden: startHidden);
    }


